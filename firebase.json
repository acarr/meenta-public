{
  "hosting": {
    "public": "_site",
    "rewrites": [
      {
        "source": "/instrument.xml",
        "function": "instrumentSitemap"
      },
      {
        "source": "/**",
        "destination": "/index.html"
      },
      {
        "source": "/app/**",
        "destination": "/app/index.html"
      }
    ]
  }
}
